<view class="page">
  <view class="container">
    <view class="table">
        <view class="tr">
            <view class="th">昵称</view>
            <view class="th">交易时间</view>
            <view class="th">类型</view>
            <view class="th">金额</view>
        </view>
        <view class="tr"  wx:for="{{list}}">
            <view class="td">{{item.nickname}}</view>
            <view class="td">{{item.createtime}}</view>
            <view class="td">{{item.remark}}</view>
            <view class="td">{{item.num}}</view>
        </view>
    </view>
</view>
</view>
